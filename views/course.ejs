<% include header %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <script src="https://code.jquery.com/jquery-3.3.1.js" integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60=" crossorigin="anonymous"></script>

    <title>CS101</title>

    <!-- Bootstrap core CSS -->
    <link href="/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="/course-pg.css" rel="stylesheet">

  </head>

  <body>

    <main role="main" class="container">

      <!--With tags instead-->
      <div class="starter-template">
        <h1 id="courseNumber"><%= mainCourse.abrCollege %> <%= mainCourse.number %></h1>
        <h2 id="courseName"><%= mainCourse.courseName %></h2>
        <br><h5 id="courseGPA">Average GPA: <%= mainCourse.avgGPA %></h5>
        <p class="lead" id="courseDescription"><%= mainCourse.courseDescription %></p>
      </div>

    </main>

    <div id="testCS101" class="table">
    </div>

    <table class="table table-striped table-bordered table-hover" align="center">
    <thead><tr>
    <th scope="col">Status</th>
    <th scope="col">Type</th>
    <th scope="col">Section</th>
    <th scope="col">Time</th>
    <th scope="col">Day</th>
    <th scope="col">Location</th>
    <th scope="col">Instructor</th></tr>

    <% sections.forEach(function(section){ %>
        <tbody><tr>
        <td><%= section.open %></td>
        <td><%= section.type %></td>
        <td><%= section.section %></td>
        <td><%= section.timeStart %> - <%= section.timeEnd %></td>
        <td><%= section.days %></td>
        <td><%= section.location %></td>
        <td><%= section.professors[0] %></td>
        </tr></tbody>
    <% }); %>

