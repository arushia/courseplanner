      <div class="container-fluid">
      <div class="row">
      <% courses.forEach(function(course){ %>
        <div class="col-md-4">
          <div class="card mb-4 box-shadow">
            <div class="card-body">
              <% var cname = course.abrCollege + " " + course.number %>
              <h3><%= cname %></h3>
              <% var gpa = course.avgGPA == -1 ? "N/A" : course.avgGPA %>
                <h6>Average GPA: <%= gpa %></h6>
                  <p class="card-text"><%= course.courseDescription %><p>
                    <div class="d-flex justify-content-between align-items-center">
                      <a class="btn btn-primary" href="" role="button">Page</a>
                      </div></div></div></div>
        <% }); %>
      </div>
      </div>

